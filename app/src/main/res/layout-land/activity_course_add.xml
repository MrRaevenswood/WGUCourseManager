<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/courseAdd"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CourseAddActivity"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Course End Date"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="210dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="Mentor Phone"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="311dp" />

    <EditText
        android:id="@+id/mentorName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Mentor Name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="453dp"
        tools:layout_editor_absoluteY="335dp" />

    <EditText
        android:id="@+id/mentorEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Mentor Email"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="453dp"
        tools:layout_editor_absoluteY="419dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:text="Course Title"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="72dp" />

    <EditText
        android:id="@+id/mentorPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Mentor Phone Number"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="445dp"
        tools:layout_editor_absoluteY="235dp" />

    <EditText
        android:id="@+id/courseTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="453dp"
        tools:layout_editor_absoluteY="66dp" />

    <EditText
        android:id="@+id/status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Status"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="453dp"
        tools:layout_editor_absoluteY="255dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:text="Course Start Date"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="Mentor Name"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="251dp" />

    <EditText
        android:id="@+id/courseEndDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Click to Add Date"
        android:inputType="textPersonName"
        android:onClick="showEndDatePickerDialog"
        tools:layout_editor_absoluteX="453dp"
        tools:layout_editor_absoluteY="184dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:text="Notes"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="347dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Status"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="270dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:text="Mentor Email"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="291dp" />

    <EditText
        android:id="@+id/courseStartDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Click to Add Date"
        android:inputType="textPersonName"
        android:onClick="showStartDatePickerDialog"
        tools:layout_editor_absoluteX="453dp"
        tools:layout_editor_absoluteY="124dp" />

    <EditText
        android:id="@+id/notes"
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:ems="10"
        android:hint="Add Notes Here"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="330dp" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="683dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:minHeight="@android:dimen/app_icon_size"
        android:theme="@style/AppTheme.NoActionBar"
        app:titleTextColor="@android:color/white" />

</android.support.constraint.ConstraintLayout>